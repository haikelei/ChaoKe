<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:orientation="vertical">
    <LinearLayout
        android:paddingLeft="20dp"
        android:paddingRight="40dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="房源状态"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <GridLayout
            android:layout_marginTop="10dp"
            android:columnCount="3"
            android:rowCount="2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="自营待租"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="自营在租"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="自营预订"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginTop="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="已失效"
                android:background="@drawable/selector_cb_ziying"
                />
        </GridLayout>
        <TextView
            android:layout_marginTop="20dp"
            android:text="状态类别"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <GridLayout
            android:layout_marginTop="10dp"
            android:columnCount="3"
            android:rowCount="2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="客户在租"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="客户将到期"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="客户转租"
                android:background="@drawable/selector_cb_ziying"
                />

        </GridLayout>
        <TextView
            android:layout_marginTop="20dp"
            android:text="处理方式"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <GridLayout
            android:layout_marginTop="10dp"
            android:columnCount="3"
            android:rowCount="2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="不续可看"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="不续不可看"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="预续订"
                android:background="@drawable/selector_cb_ziying"
                />

        </GridLayout>
        <TextView
            android:layout_marginTop="20dp"
            android:text="资产归属"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <GridLayout
            android:layout_marginTop="10dp"
            android:columnCount="3"
            android:rowCount="2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="巢客"
                android:background="@drawable/selector_cb_ziying"
                />
            <CheckBox
                android:layout_marginLeft="10dp"
                android:textColor="@drawable/selector_tc_ziying"
                android:padding="6dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="其他"
                android:background="@drawable/selector_cb_ziying"
                />
        </GridLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="40dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:text="重置"
            android:gravity="center"
            android:background="#f0eef3"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent" />
        <TextView
            android:id="@+id/btn_shaixuan"
            android:text="确定"
            android:gravity="center"
            android:textColor="#ffffff"
            android:background="@color/colorPrimary"
            android:layout_weight="1.5"
            android:layout_width="0dp"
            android:layout_height="match_parent" />
    </LinearLayout>

    <View
        android:background="#22000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>